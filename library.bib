@article{zhangNewInterfaceTracking2008,
  title = {A New Interface Tracking Method: {{The}} Polygonal Area Mapping Method},
  volume = {227},
  issn = {0021-9991},
  shorttitle = {A New Interface Tracking Method},
  abstract = {We present a new method, the polygonal area mapping (PAM) method, for tracking a non-diffusive, immiscible material interface between two materials in two-dimensional incompressible flows. This method represents material areas explicitly as piecewise polygons, traces characteristic points on polygon boundaries along pathlines and calculates new material areas inside interface cells via polygon-clippings in a discrete manner. The new method has very little spatial numerical diffusion and tracks the interface singularities naturally and accurately. In addition to high accuracy, the PAM method can be directly used on either a structured rectangular mesh or an unstructured mesh without any modifications. The mass conservation is enforced by heuristic algorithms adjusting the volume of material polygons. The results from a set of widely used benchmark tests show that the PAM method is superior to existing volume-of-fluid (VOF) methods.},
  number = {8},
  journal = {Journal of Computational Physics},
  doi = {10.1016/j.jcp.2007.12.014},
  author = {Zhang, Qinghai and Liu, Philip L. -F.},
  month = apr,
  year = {2008},
  keywords = {Interface tracking,Area mapping,Boolean set operations,Compact point set,Polygon-clipping,Volume-of-fluid methods},
  pages = {4063-4088},
  file = {/home/yeldon/Zotero/storage/5SRWBH6U/Zhang and Liu - 2008 - A new interface tracking method The polygonal are.pdf;/home/yeldon/Zotero/storage/WDLQGJM6/S0021999107005645.html}
}

@article{zhangFourthOrderAccurateFiniteVolume2012,
  title = {A {{Fourth}}-{{Order Accurate Finite}}-{{Volume Method}} with {{Structured Adaptive Mesh Refinement}} for {{Solving}} the {{Advection}}-{{Diffusion Equation}}},
  volume = {34},
  issn = {1064-8275},
  abstract = {We present a fourth-order accurate algorithm for solving Poisson's equation, the heat equation, and the advection-diffusion equation on a hierarchy of block-structured, adaptively refined grids. For spatial discretization, finite-volume stencils are derived for the divergence operator and Laplacian operator in the context of structured adaptive mesh refinement and a variety of boundary conditions; the resulting linear system is solved with a multigrid algorithm. For time integration, we couple the elliptic solver to a fourth-order accurate Runge\textendash{}Kutta method, introduced by Kennedy and Carpenter [Appl. Numer. Math., 44 (2003), pp. 139\textendash{}181], which enables us to treat the nonstiff advection term explicitly and the stiff diffusion term implicitly. We demonstrate the spatial and temporal accuracy by comparing results with analytical solutions. Because of the general formulation of the approach, the algorithm is easily extensible to more complex physical systems.},
  number = {2},
  journal = {SIAM Journal on Scientific Computing},
  doi = {10.1137/110820105},
